{
  "name": "epi-backend",
  "version": "3.5.0",
  "description": "Backend do Módulo de Gestão de EPIs v3.5 - Desenvolvido com Claude-Flow",
  "main": "dist/main.js",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "npm run test:unit && npm run test:integration && npm run test:e2e",
    "test:unit": "vitest run --dir test/unit",
    "test:integration": "TEST_TYPE=integration vitest run --dir test/integration",
    "test:watch": "vitest watch",
    "test:cov": "vitest run --coverage",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "TEST_TYPE=e2e vitest run --config ./vitest.config.e2e.ts",
    "test:db": "./scripts/test-with-db.sh",
    "test:db:unit": "./scripts/test-with-db.sh unit",
    "test:db:integration": "./scripts/test-with-db.sh integration",
    "test:db:e2e": "./scripts/test-with-db.sh e2e",
    "test:integration:full": "./scripts/run-integration-tests.sh",
    "test:real-db": "./scripts/run-integration-tests.sh",
    "seed:test": "DATABASE_URL=postgresql://postgres:postgres@localhost:5436/epi_test_db_v35?schema=public ts-node -r tsconfig-paths/register test/seeds/test-seed.ts",
    "setup": "./scripts/setup-dev-env.sh",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "docker:db": "docker-compose up -d db",
    "docker:test": "docker-compose up -d db_test redis",
    "docker:clean": "docker-compose down -v",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:deploy": "prisma migrate deploy",
    "prisma:seed": "ts-node prisma/seed.ts",
    "prisma:studio": "prisma studio",
    "prisma:test:deploy": "DATABASE_URL=postgresql://postgres:postgres@localhost:5436/epi_test_db_v35?schema=public prisma migrate deploy",
    "prisma:test:reset": "DATABASE_URL=postgresql://postgres:postgres@localhost:5436/epi_test_db_v35?schema=public prisma migrate reset --force"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "overrides": {
    "reflect-metadata": "^0.1.13"
  },
  "dependencies": {
    "@automapper/classes": "^8.8.1",
    "@automapper/core": "^8.8.1",
    "@automapper/nestjs": "^8.8.1",
    "@automapper/types": "^6.3.1",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.2.3",
    "@nestjs/core": "^10.0.0",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/swagger": "^8.1.0",
    "@prisma/client": "^5.15.1",
    "date-fns": "^4.1.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^5.0.5",
    "rxjs": "^7.8.1",
    "swagger-ui-express": "^5.0.1",
    "zod": "^3.23.8",
    "zod-validation-error": "^3.3.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/express": "^4.17.17",
    "@types/node": "^20.3.1",
    "@types/supertest": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitest/coverage-v8": "^1.6.0",
    "dotenv": "^16.4.5",
    "dotenv-cli": "^7.4.2",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "prettier": "^3.0.0",
    "prisma": "^5.15.1",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.4",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3",
    "vitest": "^1.6.0"
  }
}
